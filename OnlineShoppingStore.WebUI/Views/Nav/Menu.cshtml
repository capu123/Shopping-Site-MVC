@model IEnumerable<string>

@Html.ActionLink("Home", "List", "Product", null,
                                                    new { @class = "btn btn-block btn-info btn-lg " })

<h5> &nbsp;<span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span>  Categories</h5>
@foreach (var link in Model)
{
    @Html.RouteLink(link, new
   {
       controller = "Product",
       action = "List",
       category = link,
       page = 1
   },
       new
       {
           @class = "btn btn-block btn-default btn-lg"
           + (link == ViewBag.SelectedCategory ? " btn-primary" : "")
           
           
       }
   )
}

<h5> &nbsp;<span class="glyphicon glyphicon-user" aria-hidden="true"></span>  Admin Panel</h5>
@Html.ActionLink("Admin", "Index", "Admin", null, new { @class = "btn btn-block btn-default btn-lg" })
